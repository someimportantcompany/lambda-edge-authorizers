{
  "name": "lambda-edge-authorizers",
  "description": "Protect Cloudfront resources behind authentication services",
  "version": "0.1.0",
  "main": "./dist/",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf ./dist",
    "build:lib": "tsc",
    "build:ts": "tsc --declaration --emitDeclarationOnly"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@someimportantcompany/utils": "^1.3.1",
    "axios": "^1.6.8",
    "cookie": "^0.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "micromatch": "^4.0.5",
    "ms": "^2.1.3",
    "qs": "^6.12.1",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/micromatch": "^4.0.7",
    "@types/ms": "^0.7.34",
    "@types/qs": "^6.9.15",
    "@types/statuses": "^2.0.5"
  },
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth0": {
      "import": "./dist/lib/auth0.js",
      "require": "./dist/lib/auth0.js",
      "default": "./dist/lib/auth0.js",
      "types": "./dist/lib/auth0.d.ts"
    }
  }
}
